# DATABASE SQLSERVER
spring.datasource.driver-class-name=com.microsoft.sqlserver.jdbc.SQLServerDriver
spring.datasource.url=jdbc:sqlserver://localhost:1433;databaseName=EquipmentManagementDB
spring.datasource.username=sa
spring.datasource.password=123456

# JPA
spring.jpa.show-sql=true
spring.jpa.hibernate.ddl-auto=none
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.SQLServer2012Dialect

# Thymeleaf
spring.thymeleaf.cache=false

# Naming strategy
spring.jpa.hibernate.naming.implicit-strategy=org.hibernate.boot.model.naming.ImplicitNamingStrategyLegacyHbmImpl
spring.jpa.hibernate.naming.physical-strategy=org.springframework.boot.orm.jpa.hibernate.SpringPhysicalNamingStrategy

# Entity
spring.jackson.serialization.fail-on-empty-beans=false

# Upload file
spring.servlet.multipart.max-file-size=5MB
spring.servlet.multipart.max-request-size=5MB

# JWT
bezkoder.app.jwtSecret= bezKoderSecretKey
bezkoder.app.jwtExpirationMs= 86400000

# S3 AWS
equipmentManagement.aws.endpointUrl=https://s3em712.s3.ap-southeast-1.amazonaws.com
equipmentManagement.aws.subFolder=em
equipmentManagement.aws.bucketName=s3em712
equipmentManagement.aws.accessKey=
equipmentManagement.aws.secretKey=

# Timezone
spring.jackson.time-zone= Asia/Ho_Chi_Minh